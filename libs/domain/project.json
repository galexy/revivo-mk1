{
  "name": "domain",
  "projectType": "library",
  "sourceRoot": "libs/domain/domain",
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run --no-sync pytest libs/domain/tests/ -v --tb=short",
        "cwd": "{workspaceRoot}"
      },
      "inputs": ["{projectRoot}/**/*.py"],
      "outputs": []
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run --no-sync ruff check libs/domain/domain/ libs/domain/tests/",
        "cwd": "{workspaceRoot}"
      },
      "inputs": ["{projectRoot}/**/*.py"],
      "outputs": []
    }
  }
}
